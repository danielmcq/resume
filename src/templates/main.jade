extend default

block content
  .row
    .col-md-12
      h1#work-experience Work Experience
  .row
    each job in jobs
      include ./includes/job-header.jade

      include ./includes/duties.jade

      include ./includes/projects.jade

      include ./includes/accomplistments.jade

  .row
    .col-md-12
      h1#education Education
  .row
    .col-md-12
      table.table.table-condensed
        thead
          tr
            th School
            th Where
            th Study
            th When
        tbody
          each learn in education
            tr
              td= learn.institution
              td= learn.location
              td= learn.areaOfStudy
              td= $utils.dateRange(learn.attendStart, learn.attendEnd)
